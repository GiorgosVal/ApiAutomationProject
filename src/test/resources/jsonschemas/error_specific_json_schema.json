  "type": "object",
  "properties": {
    "type": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "status": {
      "type": "integer"
    },
    "traceId": {
      "type": "string"
    },
    "errors": {
      "type": "object",
